onStart => {
	for {
		play chomp, false, ActionLoopContinuePlay
	}
}

onClick => {
	quote "m"
	step 100
	step -100
	turn -360
	quote ""
	broadcast "quote"
}

onKey KeyQ, => {
	quote "m", "monkey", 1
}
onKey KeyW, => {
	play clap
}

onKey KeyA, => {
	play clap, ActionStop
}

onKey KeyB, => {
	play clap, ActionResume
}
