var (
	movnum int
)
onClick => {
	animate "clap"
}

onKey KeySpace, => {
	Game.Camera.On("Monkey")
}
onKey KeyB, => {
	step 10
}
onKey KeyUp, => {
	Game.Camera.Move(0,5.0)
}

onKey KeyDown, => {
	Game.Camera.Move(0,-5.0)
}

onKey KeyLeft, => {
	Game.Camera.Move(-5.0,0.0)
}

onKey KeyRight, => {
	Game.Camera.Move(0.0,5.0)
}


onMoving mi => {

}

onTurning ti=> {
	Crocodile.SetHeading ti.newDir
}
